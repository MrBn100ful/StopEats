{
    "name": "StopEats",
    "description": "Limit your UberEats habit",
    "version": "1.0",
    "author": "MrBn100ful",
    "manifest_version": 3,
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
      },
      "content_scripts": [
		{
		 "matches": ["https://www.ubereats.com/fr/checkout*"],
		 "js": ["stopeats.js"]
		}
	]
}